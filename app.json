{
  "expo": {
    "name": "muscle-ai",
    "slug": "muscle-ai",
    "version": "6.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "muscleai",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1A1A1A"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.muscleai.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#1A1A1A"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.muscleai.app",
      "versionCode": 10,
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "muscleai"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "owner": "tester-email2",
    "plugins": [
      "./plugins/withAndroidBillingPermission.js",
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "minSdkVersion": 24,
            "kotlinVersion": "2.0.0"
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "f0ce67ed-aa61-4871-9b71-53c33383be3c"
      }
    }
  }
}
